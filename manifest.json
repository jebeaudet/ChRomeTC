{
	"manifest_version": 2,

	"name": "ChRomeTC",
	"description": "Get the real time delay before the next bus arrives.",
	"version": "0.1",

	"browser_action": {
		"default_icon": "img/bus_512x512.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"storage",
		"http://*/*",
		"https://*/*"
	]
}